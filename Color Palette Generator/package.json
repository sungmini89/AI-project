{
  "name": "ai-color-palette-generator",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "ANALYZE=true npm run build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:accessibility": "playwright test tests/accessibility.spec.ts",
    "test:performance": "playwright test tests/performance.spec.ts",
    "test:performance:color-generation": "playwright test tests/performance.spec.ts --grep=\"color.*generation\"",
    "test:performance:harmony-rules": "playwright test tests/performance.spec.ts --grep=\"harmony\"",
    "test:performance:keyword-mapping": "playwright test tests/performance.spec.ts --grep=\"keyword\"",
    "test:performance:color-conversion": "playwright test tests/performance.spec.ts --grep=\"conversion\"",
    "test:performance:vibrant-extraction": "playwright test tests/performance.spec.ts --grep=\"vibrant\"",
    "test:performance:canvas-processing": "playwright test tests/performance.spec.ts --grep=\"canvas\"",
    "test:performance:memory-leaks": "playwright test tests/performance.spec.ts --grep=\"memory.*leak\"",
    "test:performance:gc-optimization": "playwright test tests/performance.spec.ts --grep=\"gc\"",
    "test:performance:api-colormind": "playwright test tests/performance.spec.ts --grep=\"api.*colormind\"",
    "test:performance:api-thecolorapi": "playwright test tests/performance.spec.ts --grep=\"api.*thecolor\"",
    "test:performance:api-offline": "playwright test tests/performance.spec.ts --grep=\"api.*offline\"",
    "test:performance:fallback-speed": "playwright test tests/performance.spec.ts --grep=\"fallback\"",
    "test:image-extraction": "playwright test tests/performance.spec.ts --grep=\"image\"",
    "test:memory-usage": "playwright test tests/performance.spec.ts --grep=\"memory\"",
    "test:accessibility-report": "playwright test tests/accessibility.spec.ts --reporter=html",
    "test:e2e:color-accuracy": "playwright test tests/color-generation.spec.ts --grep=\"accuracy\"",
    "test:e2e:mobile-colors": "playwright test tests/color-generation.spec.ts --grep=\"mobile\"",
    "lighthouse": "lhci autorun",
    "lighthouse:desktop": "lhci autorun --config=lighthouse-config.js",
    "size-limit": "npm run build && du -sh dist/*"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@types/react-router-dom": "^5.3.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.263.1",
    "node-vibrant": "^3.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.8.2",
    "tailwind-merge": "^1.14.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "imagemin-svgo": "^11.0.1",
    "imagemin-webp": "^8.0.0",
    "postcss": "^8.4.27",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-bundle-analyzer": "^1.2.3",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-pwa": "^1.0.3",
    "workbox-cli": "^7.3.0"
  }
}
