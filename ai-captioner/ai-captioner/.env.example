# 개발 중 목데이터 사용 여부 (true/false)
VITE_USE_MOCK_DATA=true

# API 모드: Offline | Free | Premium | Custom
VITE_API_MODE=Offline

# Free API 모드 엔드포인트/키 (예: 본인 프록시/테스트 서버)
VITE_FREE_API_BASE_URL="https://your-free-api.example.com"
VITE_FREE_API_KEY=""

# Custom(API Key 사용자 입력) 모드일 때 기본 베이스 URL
VITE_CUSTOM_API_BASE_URL=""

# Ollama 오프라인 서버 설정
VITE_OLLAMA_BASE_URL="http://localhost:11434"
VITE_OLLAMA_MODEL="llava:latest"

# 레이트 리미트 설정
VITE_RATE_LIMIT_PER_MINUTE=30

# Free 모드 한도
VITE_FREE_DAILY_LIMIT=50
VITE_FREE_MONTHLY_LIMIT=500
